         </div>
         <div class="progress" style="margin-top:20px;">
          <div class=" progress-bar bg-success" role="progressbar" style="width:55%;" aria-valuenow="55%" aria-valuemin="0" aria-valuemax="100">55%</div>
          
        </div>
        <p>of shipped orders</p>
        
        </div>
      </div>
     </div>
     <div class="col-6 col-md-3"  style="margin-top:5px;">
      <div class="card" style="width:13rem;height:10rem;">
        <div class="card-body">
          <h5 class="card-title">Sales</h5>
          <div  style="margin-top:35px;">
            <h6 class="card-title mb-2 text-muted" style="margin-top:15px;">Card subtitle</h6>
           </div>
        
        </div>
      </div>
     </div>
   </div>
 </div>
 <!----------------------------->
 <div class="container" style="margin-top:20px;">
     <div class="accordion" id="accordionPanelsStayOpenExample">
  <div class="accordion-item">
    <h2 class="accordion-header" id="panelsStayOpen-headingOne">
      <button class="accordion-button" type="button" data-bs-toggle="collapse" data-bs-target="#panelsStayOpen-collapseOne" aria-expanded="true" aria-controls="panelsStayOpen-collapseOne">
        Recieved Orders
      </button>
    </h2>
    <div id="panelsStayOpen-collapseOne" class="accordion-collapse collapse show" aria-labelledby="panelsStayOpen-headingOne">
      <div class="accordion-body">
                         {{#if result}}
    {{#each result}}
      <div class="card" style="margin-top:10px">
        <div class="card-header">
          item name: {{this.item_name}}
        </div>
        <div class="card-body">
          <blockquote class="blockquote mb-0">
            <div class="row">
                <div class="col-3">
                    <img src="{{{this.image}}}" style="width:100px;height:100px" alt="">
                    <h6>Order Id:{{this.order_id}}</h6>
                </div>
                <div class="col-6">
                    
                    <h6 style="color:rgb(19, 17, 17)"> Rs.{{this.price}}.00</h6>
                    <h6>Item Id:{{this.item_id}}</h6>
                    <h6>Purchased Qty:{{this.quantity}}</h6>
                    <h6>Shipping address:{{this.customer_address}}</h6>
                </div>
                <div class="col-3">
                    
                  <a class="btn btn-outline-info" href="/ship/{{{this.order_id}}}"><i class="fa fa-check-circle-o" aria-hidden="true"></i>  Confirm Shipment</a>
                    
                </div>
            </div>
            
          </blockquote>
        </div>
      </div>
      {{/each}}
      {{/if}}
        
      </div>
    </div>
  </div>
  
  
</div>

